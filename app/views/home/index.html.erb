<h1 align="center">ROOSTER</h1>
<br><br>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <td><h3>Calendar ID</h3></td>
      <% @events_gist_headings.each do |heading| %>
        <td><h3><%= heading %></h3></td>
      <% end %>
    </tr>
  </thead>

  <tbody>
  <% @events_gist.each do |calendar_id, event_gist_hash| %>
      <tr>
        <td><h5><%= calendar_id %></h5></td>
        <% event_gist_hash.values.each do |column| %>
            <td><h5><%= column %></h5></td>
        <% end %>
      </tr>
  <% end %>
  </tbody>

</table>
